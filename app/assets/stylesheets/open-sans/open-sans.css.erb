/*(1) OpenSans-Regular-webfont
-----------------------------------------------*/
@font-face {
  font-family: "Open Sans Web";
    /* src: url('<%= asset_path("font-awesome/fontawesome-webfont.eot") %>?v=4.1.0'); */
  src: url('<%= asset_path("open-sans/OpenSans-Regular-webfont.eot") %>');
  src: url('<%= asset_path("open-sans/OpenSans-Regular-webfont.eot") %>?#iefix'),
url('<%= asset_path("open-sans/OpenSans-Regular-webfont.woff") %>') format("woff"),
url('<%= asset_path("open-sans/OpenSans-Regular-webfont.ttf") %>') format("truetype"),
url('<%= asset_path("open-sans/OpenSans-Regular-webfont.svg") %>#Open Sans Web') format("svg");
  font-weight: normal;
  font-style: normal;
}
